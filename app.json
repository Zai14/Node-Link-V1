{
  "expo": {
    "name": "NodeLink",
    "slug": "NodeLink",
    "version": "1.0.0",
    "orientation": "portrait",
     "icon": "./assets/images/app-logo-24px.png",   
    "scheme": "nodelink",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "largeIcon": "./assets/images/app-logo-192px.png",
    "notification": {
      "icon": "./assets/images/app-logo-24px.png",
      "color": "#FF6600",
      "android": {
        "largeIcon": "./assets/images/app-logo-192px.png"
      }
    },

    "ios": {
      "supportsTablet": true
    },

    "android": {
      "edgeToEdgeEnabled": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },

    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },

    "plugins": [
      ["expo-notifications"],
      "expo-router",
      "expo-audio",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],

    "experiments": {
      "typedRoutes": true
    }
  }
}
